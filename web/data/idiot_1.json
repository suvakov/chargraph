{
    "characters": [
        {
            "names": [
                "Prince Lef Nicolaievitch Muishkin",
                "the prince"
            ],
            "description": "A young man of about twenty-seven, recently returned from Switzerland where he was treated for epilepsy. He is considered by many to be naive and simple-minded, but possesses great insight and a kind heart.",
            "main_character": true,
            "id": 1.0,
            "common_name": "Muishkin"
        },
        {
            "names": [
                "Parfen Rogojin",
                "Rogojin"
            ],
            "description": "A passionate and impulsive young man of about twenty-seven, who is heir to a large fortune. He is deeply in love with Nastasia Philipovna, and is prone to violence.",
            "main_character": true,
            "id": 2.0,
            "common_name": "Rogojin"
        },
        {
            "names": [
                "Lebedeff"
            ],
            "description": "A shabbily dressed man of about forty, who is a clerk. He is a gossiper and a sycophant, but possesses a considerable knowledge of the world.",
            "main_character": false,
            "id": 3.0,
            "common_name": "Lebedeff"
        },
        {
            "names": [
                "General Ivan Fedorovitch Epanchin",
                "the general"
            ],
            "description": "A wealthy and influential man of about fifty-five, who is a general in the Russian army. He is the father of three daughters, and has a passion for gambling.",
            "main_character": true,
            "id": 4.0,
            "common_name": "Epanchin"
        },
        {
            "names": [
                "Elizabetha Prokofievna",
                "Madame Epanchin"
            ],
            "description": "The wife of General Epanchin, and a woman of strong will and great pride. She comes of the princely stock of Muishkin.",
            "main_character": true,
            "id": 5.0,
            "common_name": "Mrs. Epanchin"
        },
        {
            "names": [
                "Alexandra Epanchin"
            ],
            "description": "The eldest daughter of General Epanchin, about twenty-five years old. She is musical, quiet and well educated.",
            "main_character": false,
            "id": 6.0,
            "common_name": "Alexandra"
        },
        {
            "names": [
                "Adelaida Epanchin"
            ],
            "description": "The middle daughter of General Epanchin, about twenty-three years old. She is a clever artist.",
            "main_character": false,
            "id": 7.0,
            "common_name": "Adelaida"
        },
        {
            "common_name": "Aglaya",
            "description": "The youngest and most beautiful daughter of General Epanchin, about twenty years old. She is intelligent and headstrong.",
            "main_character": true,
            "id": 8.0,
            "names": [
                "Aglaya Epanchin"
            ]
        },
        {
            "common_name": "Gania",
            "description": "A handsome young man of about twenty-eight, who is General Epanchin's secretary. He is ambitious, and seeks to marry Nastasia Philipovna for her money.",
            "main_character": true,
            "id": 9.0,
            "names": [
                "Gavrila Ardalionovitch Ivolgin",
                "Gania"
            ]
        },
        {
